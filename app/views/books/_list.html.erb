<h2>User info</h2>
 <%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg' , fallback: "no-image.jpg"  %>
<table class="table">
     <tbody><tr></tr>
         <tr>
    	     <th><%= user.name %></th>
         </tr>
      <tr>
    	     <th><%= user.introduction %></th>
    	     <th></th>
      </tr>
     </tbody>
</table>
 <%= link_to "edit", edit_user_path(current_user) %>
<div class="row">
  <a class="col-xs-12 btn btn-default glyphicon glyphicon-wrench edit_user_6074" href="/users/6074/edit"></a>
</div>
<h1>New book</h1>
  <%= form_for book, url: books_path do |f| %>
     <h4>Title</h4>
         <%= f.text_field :title %>
     <h4>Body</h4>
        <%= f.text_area :body %>
        <%= f.submit 'Create Book' %>
  <% end %>